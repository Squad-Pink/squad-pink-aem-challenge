<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Certificates Container"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured"
               sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <tab-1 jcr:primaryType="nt:unstructured"
                      jcr:title="Input com lista"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <certificatesContainer composite="{Boolean}true"
                                                  fieldLabel="Adicione e edite os input com lista"
                                                  jcr:primaryType="nt:unstructured"
                                                  sling:resourceType="granite/ui/components/coral/foundation/form/multifield">
                              <field cq-msm-lockable="./certificatesContainer"
                                     jcr:primaryType="nt:unstructured"
                                     name="./certificatesContainer"
                                     sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                 <items jcr:primaryType="nt:unstructured">
                                    <certificatesInputType defaultValue="Escolha o tipo do input"
                                                           fieldLabel="Escolha o tipo do input"
                                                           jcr:primaryType="nt:unstructured"
                                                           name="./certificatesInputType"
                                                           sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                           value="false">
                                                           <items jcr:primaryType="nt:unstructured">
                                                            <option1 jcr:primaryType="nt:unstructured" text="text" value="type1" />
                                                            <option2 jcr:primaryType="nt:unstructured" text="url" value="type2" />
                                                            <option3 jcr:primaryType="nt:unstructured" text="tel" value="type3" />
                                                            <option4 jcr:primaryType="nt:unstructured" text="email" value="type4" />                                                               
                                                           </items>
                                    </certificatesInputType>
                                    <certificatesLabelText fieldLabel="Digite o texto da label"
                                                           jcr:primaryType="nt:unstructured"
                                                           name="./certificatesLabelText"
                                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesLabelColor fieldLabel="Escolha a cor da label"
                                                            jcr:primaryType="nt:unstructured"
                                                            name="./certificatesLabelColor"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <certificatesPlaceholderText fieldLabel="Digite o texto do placeholder"
                                                                 jcr:primaryType="nt:unstructured"
                                                                 name="./certificatesPlaceholderText"
                                                                 sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesBorderColor fieldLabel="Escolha a cor da borda do input"
                                                             jcr:primaryType="nt:unstructured"
                                                             name="./certificatesBorderColor"
                                                             sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <certificatesSplitText fieldLabel="Digite o texto do splitButton"
                                                           jcr:primaryType="nt:unstructured"
                                                           name="./certificatesSplitText"
                                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesBtnColor fieldLabel="Escolha a cor dos botões"
                                                          jcr:primaryType="nt:unstructured"
                                                          name="./certificatesBtnColor"
                                                          sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <certificatesBtnTextColor fieldLabel="Escolha a cor dos textos dos botões"
                                                              jcr:primaryType="nt:unstructured"
                                                              name="./certificatesBtnTextColor"
                                                              sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <certificatesSplitFirstIconResType jcr:primaryType="nt:unstructured"
                                                                       name="./certificatesSplitFirstIcon/sling:resourceType"
                                                                       sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                       value="core/wcm/components/image/v2/image"/>
                                    <certificatesSplitFirstIcon allowUpload="{Boolean}false"
                                                                autoStart="{Boolean}false"
                                                                class="cq-droptarget"
                                                                fieldLabel="Escolha o ícone a esquerda do texto do splitbutton"
                                                                fileNameParameter="./certificatesSplitFirstIcon/fileName"
                                                                fileReferenceParameter="./certificatesSplitFirstIcon/fileReference"
                                                                jcr:primaryType="nt:unstructured"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                                multiple="{Boolean}false"
                                                                name="./certificatesSplitFirstIcon/file"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                title="Drag to select image"
                                                                uploadUrl="${suffix.path}"
                                                                useHTML5="{Boolean}true"/>
                                    <certificatesSplitSecondIconResType jcr:primaryType="nt:unstructured"
                                                                        name="./certificatesSplitSecondIcon/sling:resourceType"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        value="core/wcm/components/image/v2/image"/>
                                    <certificatesSplitSecondIcon allowUpload="{Boolean}false"
                                                                 autoStart="{Boolean}false"
                                                                 class="cq-droptarget"
                                                                 fieldLabel="Escolha o ícone a direita do texto do splitbutton"
                                                                 fileNameParameter="./certificatesSplitSecondIcon/fileName"
                                                                 fileReferenceParameter="./certificatesSplitSecondIcon/fileReference"
                                                                 jcr:primaryType="nt:unstructured"
                                                                 mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                                 multiple="{Boolean}false"
                                                                 name="./certificatesSplitSecondIcon/file"
                                                                 sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                 title="Drag to select image"
                                                                 uploadUrl="${suffix.path}"
                                                                 useHTML5="{Boolean}true"/>
                                    <certificatesSplitDivColor fieldLabel="Escolha a cor de fundo da lista"
                                                               jcr:primaryType="nt:unstructured"
                                                               name="./certificatesSplitDivColor"
                                                               sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <certificatesMoreText fieldLabel="Digite o texto do botão de adicionar itens na lista"
                                                          jcr:primaryType="nt:unstructured"
                                                          name="./certificatesMoreText"
                                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesBtnFirstIconResType jcr:primaryType="nt:unstructured"
                                                                     name="./certificatesBtnFirstIcon/sling:resourceType"
                                                                     sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                     value="core/wcm/components/image/v2/image"/>
                                    <certificatesBtnFirstIcon allowUpload="{Boolean}false"
                                                              autoStart="{Boolean}false"
                                                              class="cq-droptarget"
                                                              fieldLabel="Escolha o ícone a esquerda do texto do botão de adicionar itens na lista"
                                                              fileNameParameter="./certificatesBtnFirstIcon/fileName"
                                                              fileReferenceParameter="./certificatesBtnFirstIcon/fileReference"
                                                              jcr:primaryType="nt:unstructured"
                                                              mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                              multiple="{Boolean}false"
                                                              name="./certificatesBtnFirstIcon/file"
                                                              sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                              title="Drag to select image"
                                                              uploadUrl="${suffix.path}"
                                                              useHTML5="{Boolean}true"/>
                                    <certificatesBtnSecondIconResType jcr:primaryType="nt:unstructured"
                                                                      name="./certificatesBtnSecondIcon/sling:resourceType"
                                                                      sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                      value="core/wcm/components/image/v2/image"/>
                                    <certificatesBtnSecondIcon allowUpload="{Boolean}false"
                                                               autoStart="{Boolean}false"
                                                               class="cq-droptarget"
                                                               fieldLabel="Escolha o ícone a direita do texto do botão de adicionar itens na lista"
                                                               fileNameParameter="./certificatesBtnSecondIcon/fileName"
                                                               fileReferenceParameter="./certificatesBtnSecondIcon/fileReference"
                                                               jcr:primaryType="nt:unstructured"
                                                               mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                               multiple="{Boolean}false"
                                                               name="./certificatesBtnSecondIcon/file"
                                                               sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                               title="Drag to select image"
                                                               uploadUrl="${suffix.path}"
                                                               useHTML5="{Boolean}true"/>
                                    <certificatesErrorText fieldLabel="Digite o texto da mensagem de erro"
                                                           jcr:primaryType="nt:unstructured"
                                                           name="./certificatesErrorText"
                                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesLimitErrorText fieldLabel="A lista tem um limite de 5 itens, digite o texto da mensagem de erro caso haja tentativa de ultrapassá-lo"
                                                                jcr:primaryType="nt:unstructured"
                                                                name="./certificatesLimitErrorText"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <certificatesErrorColor fieldLabel="Escolha a cor das mensagens de erro"
                                                            jcr:primaryType="nt:unstructured"
                                                            name="./certificatesErrorColor"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                 </items>
                              </field>
                           </certificatesContainer>
                        </items>
                     </column>
                  </items>
               </tab-1>
               <tab-2 jcr:primaryType="nt:unstructured"
                      jcr:title="Inputs"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <dataCertificatesContainer composite="{Boolean}true"
                                                      fieldLabel="Adicione e edite os inputs"
                                                      jcr:primaryType="nt:unstructured"
                                                      sling:resourceType="granite/ui/components/coral/foundation/form/multifield">
                              <field cq-msm-lockable="./dataCertificatesContainer"
                                     jcr:primaryType="nt:unstructured"
                                     name="./dataCertificatesContainer"
                                     sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                 <items jcr:primaryType="nt:unstructured">
                                    <inputType defaultValue="Escolha o tipo do input"
                                               fieldLabel="Escolha o tipo do input"
                                               jcr:primaryType="nt:unstructured"
                                               name="./inputType"
                                               sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                               value="false">
                                               <items jcr:primaryType="nt:unstructured">
                                                   <option1 jcr:primaryType="nt:unstructured" text="text" value="type1" />
                                                   <option2 jcr:primaryType="nt:unstructured" text="url" value="type2" />
                                                   <option3 jcr:primaryType="nt:unstructured" text="tel" value="type3" />
                                                   <option4 jcr:primaryType="nt:unstructured" text="email" value="type4" />     
                                                   <option5 jcr:primaryType="nt:unstructured" text="checkbox" value="type5" />                                                        
                                                </items>
                                    </inputType>
                                    <inputLabelText fieldLabel="Digite o texto da label"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./inputLabelText"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <inputLabelColor fieldLabel="Escolha a cor da label"
                                                     jcr:primaryType="nt:unstructured"
                                                     name="./inputLabelColor"
                                                     sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <inputErrorText fieldLabel="Digite o texto da mensagem de erro"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./inputErrorText"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <inputColorError fieldLabel="Escolha a cor da mensagem de erro"
                                                     jcr:primaryType="nt:unstructured"
                                                     name="./inputColorError"
                                                     sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                    <inputPlaceholderText fieldLabel="Digite o texto do placeholder"
                                                          jcr:primaryType="nt:unstructured"
                                                          name="./inputPlaceholderText"
                                                          sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                    <inputBorderColor fieldLabel="Escolha a cor da borda do input"
                                                      jcr:primaryType="nt:unstructured"
                                                      name="./inputBorderColor"
                                                      sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                                 </items>
                              </field>
                           </dataCertificatesContainer>
                        </items>
                     </column>
                  </items>
               </tab-2>
               <tab-3 jcr:primaryType="nt:unstructured"
                      jcr:title="Submit Button"
                      sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured"
                             sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <submitBtnTitle cq-msm-lockable="./submitBtnTitle"
                                           fieldLabel="Digite o texto do botão"
                                           jcr:primaryType="nt:unstructured"
                                           name="./submitBtnTitle"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                           <submitBtnColor cq-msm-lockable="./submitBtnColor"
                                           fieldLabel="Escolha a cor do botão"
                                           jcr:primaryType="nt:unstructured"
                                           name="./submitBtnColor"
                                           sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                           <submitColorText cq-msm-lockable="./submitColorText"
                                            fieldLabel="Escolha a cor do texto do botão"
                                            jcr:primaryType="nt:unstructured"
                                            name="./submitColorText"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"/>
                           <submitFirstIconResType jcr:primaryType="nt:unstructured"
                                                   name="./submitFirstIcon/sling:resourceType"
                                                   sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                   value="core/wcm/components/image/v2/image"/>
                           <submitFirstIcon allowUpload="{Boolean}false"
                                            autoStart="{Boolean}false"
                                            class="cq-droptarget"
                                            cq-msm-lockable="./submitFirstIcon/file"
                                            fieldLabel="Escolha o ícone a esquerda do texto do botão "
                                            fileNameParameter="./submitFirstIcon/fileName"
                                            fileReferenceParameter="./submitFirstIcon/fileReference"
                                            jcr:primaryType="nt:unstructured"
                                            mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                            multiple="{Boolean}false"
                                            name="./submitFirstIcon/file"
                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                            title="Drag to select image"
                                            uploadUrl="${suffix.path}"
                                            useHTML5="{Boolean}true"/>
                           <submitSecondIconResType jcr:primaryType="nt:unstructured"
                                                    name="./submitSecondIcon/sling:resourceType"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                    value="core/wcm/components/image/v2/image"/>
                           <submitSecondIcon allowUpload="{Boolean}false"
                                             autoStart="{Boolean}false"
                                             class="cq-droptarget"
                                             cq-msm-lockable="./submitSecondIcon/file"
                                             fieldLabel="Escolha o ícone a direita do texto do botão"
                                             fileNameParameter="./submitSecondIcon/fileName"
                                             fileReferenceParameter="./submitSecondIcon/fileReference"
                                             jcr:primaryType="nt:unstructured"
                                             mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                             multiple="{Boolean}false"
                                             name="./submitSecondIcon/file"
                                             sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                             title="Drag to select image"
                                             uploadUrl="${suffix.path}"
                                             useHTML5="{Boolean}true"/>
                        </items>
                     </column>
                  </items>
               </tab-3>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>
