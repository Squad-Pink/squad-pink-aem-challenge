<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0" jcr:primaryType="nt:unstructured" jcr:title="Social Container" sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
      <items jcr:primaryType="nt:unstructured">
         <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs">
            <items jcr:primaryType="nt:unstructured">
               <tab-1 jcr:primaryType="nt:unstructured" jcr:title="Inputs" sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" />
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <socialContainer composite="{Boolean}true" fieldLabel="Adicione e edite os inputs" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield">
                              <field cq-msm-lockable="./socialContainer" jcr:primaryType="nt:unstructured" name="./socialContainer" sling:resourceType="granite/ui/components/foundation/form/fieldset">
                                 <items jcr:primaryType="nt:unstructured">
                                    <inputType defaultValue="Escolha o tipo do input" fieldLabel="Escolha o tipo do input" jcr:primaryType="nt:unstructured" name="./inputType" sling:resourceType="granite/ui/components/coral/foundation/form/select" value="false">
                                       <items jcr:primaryType="nt:unstructured">
                                          <option1 jcr:primaryType="nt:unstructured" text="text" value="type1" />
                                          <option2 jcr:primaryType="nt:unstructured" text="url" value="type2" />
                                          <option3 jcr:primaryType="nt:unstructured" text="tel" value="type3" />
                                          <option4 jcr:primaryType="nt:unstructured" text="email" value="type4" />
                                          <option5 jcr:primaryType="nt:unstructured" text="checkbox" value="type5" />
                                       </items>
                                    </inputType>
                                    <inputLabelText fieldLabel="Digite o texto da label" jcr:primaryType="nt:unstructured" name="./inputLabelText" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                                    <inputLabelColor fieldLabel="Escolha a cor da label" jcr:primaryType="nt:unstructured" name="./inputLabelColor" sling:resourceType="granite/ui/components/coral/foundation/form/colorfield" />
                                    <inputErrorText fieldLabel="Digite o texto da mensagem de erro" jcr:primaryType="nt:unstructured" name="./inputErrorText" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                                    <inputColorError fieldLabel="Escolha a cor da mensagem de erro" jcr:primaryType="nt:unstructured" name="./inputColorError" sling:resourceType="granite/ui/components/coral/foundation/form/colorfield" />
                                    <inputPlaceholderText fieldLabel="Digite o texto do placeholder" jcr:primaryType="nt:unstructured" name="./inputPlaceholderText" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                                    <inputBorderColor fieldLabel="Escolha a cor da borda do input" jcr:primaryType="nt:unstructured" name="./inputBorderColor" sling:resourceType="granite/ui/components/coral/foundation/form/colorfield" />
                                 </items>
                              </field>
                           </socialContainer>
                        </items>
                     </column>
                  </items>
               </tab-1>
               <tab-2 jcr:primaryType="nt:unstructured" jcr:title="Button" sling:resourceType="granite/ui/components/coral/foundation/container">
                  <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" />
                  <items jcr:primaryType="nt:unstructured">
                     <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                           <submitBtnTitle cq-msm-lockable="./submitBtnTitle" fieldLabel="Digite o texto do botão" jcr:primaryType="nt:unstructured" name="./submitBtnTitle" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" />
                           <submitBtnColor cq-msm-lockable="./submitBtnColor" fieldLabel="Escolha a cor do botão" jcr:primaryType="nt:unstructured" name="./submitBtnColor" sling:resourceType="granite/ui/components/coral/foundation/form/colorfield" />
                           <submitColorText cq-msm-lockable="./submitColorText" fieldLabel="Escolha a cor do texto do botão" jcr:primaryType="nt:unstructured" name="./submitColorText" sling:resourceType="granite/ui/components/coral/foundation/form/colorfield" />
                           <submitFirstIconResType jcr:primaryType="nt:unstructured" name="./submitFirstIcon/sling:resourceType" sling:resourceType="granite/ui/components/coral/foundation/form/hidden" value="core/wcm/components/image/v2/image" />
                           <submitFirstIcon allowUpload="{Boolean}false" autoStart="{Boolean}false" class="cq-droptarget" cq-msm-lockable="./submitFirstIcon/file" fieldLabel="Escolha o ícone a esquerda do texto do botão " fileNameParameter="./submitFirstIcon/fileName" fileReferenceParameter="./submitFirstIcon/fileReference" jcr:primaryType="nt:unstructured" mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]" multiple="{Boolean}false" name="./submitFirstIcon/file" sling:resourceType="cq/gui/components/authoring/dialog/fileupload" title="Drag to select image" uploadUrl="${suffix.path}" useHTML5="{Boolean}true" />
                           <submitSecondIconResType jcr:primaryType="nt:unstructured" name="./submitSecondIcon/sling:resourceType" sling:resourceType="granite/ui/components/coral/foundation/form/hidden" value="core/wcm/components/image/v2/image" />
                           <submitSecondIcon allowUpload="{Boolean}false" autoStart="{Boolean}false" class="cq-droptarget" cq-msm-lockable="./submitSecondIcon/file" fieldLabel="Escolha o ícone a direita do texto do botão" fileNameParameter="./submitSecondIcon/fileName" fileReferenceParameter="./submitSecondIcon/fileReference" jcr:primaryType="nt:unstructured" mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]" multiple="{Boolean}false" name="./submitSecondIcon/file" sling:resourceType="cq/gui/components/authoring/dialog/fileupload" title="Drag to select image" uploadUrl="${suffix.path}" useHTML5="{Boolean}true" />
                        </items>
                     </column>
                  </items>
               </tab-2>
            </items>
         </tabs>
      </items>
   </content>
</jcr:root>